{
  "name": "react-i18n-viz",
  "version": "1.0.0",
  "description": "Visualization of i18n-strings for react",
  "license": "MIT",
  "scripts": {
    "test:watch": "jest --watch",
    "test": "jest",
    "prepare": "npm run build",
    "clean": "rimraf lib",
    "build": "npm run clean && babel ./src --out-dir ./lib",
    "start": "npm run clean && babel ./src --watch --out-dir ./lib",
    "lint": "eslint .",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "author": "Timo MÃ¤mecke <timo@railslove.com>",
  "homepage": "https://github.com/railslove/react-i18n-viz",
  "repository": {
    "type": "git",
    "url": "https://github.com/railslove/react-i18n-viz"
  },
  "bugs": {
    "url": "https://github.com/railslove/react-i18n-viz/issues"
  },
  "keywords": [
    "i18n",
    "intl",
    "react",
    "internationalization",
    "tooltip",
    "visualization",
    "react-intl",
    "react-i18next"
  ],
  "main": "lib/index.js",
  "files": ["lib"],
  "sideEffects": false,
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "react-portal": "^4.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "gh-pages": "^1.1.0",
    "i18next": "^11.1.1",
    "jest": "^22.4.3",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-i18next": "^7.5.1",
    "react-intl": "^2.4.0"
  },
  "jest": {
    "setupFiles": ["<rootDir>/setupTests.js"],
    "modulePathIgnorePatterns": ["<rootDir>/example/node_modules"]
  }
}
